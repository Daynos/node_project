{# /views/output/xml/page.twig #}
 <page>
    <title>{{ title }}</title>
    <revision>
      <timestamp>{{ timestamp }}</timestamp>
      <contributor>
        <username>Daynos de Nory</username>
      </contributor>
      <comment>Import</comment>
      <text xml:space="preserve">{% block content %}{% endblock %}</text>
      <model>wikitext</model>
      <format>text/x-wiki</format>
    </revision>
  </page>